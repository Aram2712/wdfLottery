<title><%- title %></title>
<link rel="stylesheet" href="/css/boughtRaffleInfo.css">
<link rel="icon" href="/logos/logo.svg">
<div class="boughtTicketsBody">
    <table class="table table-striped table-dark">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Full name:</th>
            <th scope="col">Serial NO:</th>
            <th scope="col">Ticket amount:</th>
            <th scope="col">Status:</th>
        </tr>
        </thead>
        <% result.map(info => {%>
            <tbody class="userTicketInfo">
                <tr>
                    <th scope="row"><%- info.userId%></th>
                    <td><%- info.firstName + ' ' + info.lastName%></td>
                    <td><%- info.drawSerialNO%></td>
                    <td><%- info.ticketAmount%></td>
                    <td><%- info.orderStatus%></td>
                </tr>
            </tbody>
        <%}) %>

    </table>
</div>

<%- include('../admin/firstPageForAdminLogin.ejs') %>